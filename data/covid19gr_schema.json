{
  "profile": "tabular-data-package",
  "resources": [
    {
      "name": "covid_checks",
      "path": "csv/covid_checks.csv",
      "profile": "tabular-data-resource",
      "schema": {
        "fields": [
          {
            "name": "date",
            "type": "date",
            "format": "YYYY-MM-DD",
            "title": "Date",
            "description": "date of the record"
          },
          {
            "name": "checks",
            "type": "integer",
            "format": "default",
            "title": "Checks (NTA)",
            "description": "total checks reported by the National Transparency Agency Report"
          },
          {
            "name": "checks_validation",
            "type": "integer",
            "format": "default",
            "title": "Checks (Validation)",
            "description": "total checks as a sum of the prefectures checks"
          },
          {
            "name": "violations",
            "type": "integer",
            "format": "default",
            "title": "Violations",
            "description": "total violations"
          },
          {
            "name": "fines_eur",
            "type": "number",
            "format": "default",
            "title": "Fines",
            "description": "total amount in EUR currency charged as a result of COVID-19 violations"
          },
          {
            "name": "business_closed",
            "type": "integer",
            "format": "default",
            "title": "Businesses Closed",
            "description": "total number of businesses that stopped working as a result of violating the COVID-19 measures"
          },
          {
            "name": "by_police",
            "type": "integer",
            "format": "default",
            "title": "Checks by Police",
            "description": "checks performed by the Hellenic Police force"
          },
          {
            "name": "by_others",
            "type": "integer",
            "format": "default",
            "title": "Checks by Others ",
            "description": "checks performed by the Hellenic Police force"
          },
          {
            "name": "calls_1520",
            "type": "integer",
            "format": "default",
            "title": "Calls at 1520",
            "description": "calls made to the COVID-19 related phone number 1520"
          },
          {
            "name": "v_validation",
            "type": "integer",
            "format": "default",
            "title": "Violations percentage validation",
            "description": "field used to validate whether the percentages of types of violations sum to 100% [0, 100]"
          },
          {
            "name": "v_masks",
            "type": "integer",
            "format": "default",
            "title": "Mask Violations (percentage)",
            "description": "percentage of mask violations [0, 100]"
          },
          {
            "name": "v_distancing",
            "type": "string",
            "format": "default",
            "title": "Social Distancing Violations (percentage)",
            "description": "percentage of social distancing violations [0, 100]"
          },
          {
            "name": "v_quarantine",
            "type": "integer",
            "format": "default",
            "title": "Quarantine Violations (percentage)",
            "description": "percentage of quarantine violations [0, 100]"
          },
          {
            "name": "v_other",
            "type": "integer",
            "format": "default",
            "title": "Other Violations (percentage)",
            "description": "percentage of other violations [0, 100]"
          },
          {
            "name": "pe01",
            "type": "integer",
            "format": "default",
            "title": "Checks in region PE01",
            "description": "checks performed in East Macedonia and Thrace (PE01)"
          },
          {
            "name": "pe02",
            "type": "integer",
            "format": "default",
            "title": "Checks in region PE02",
            "description": "checks performed in  Central Macedonia (PE02)"
          },
